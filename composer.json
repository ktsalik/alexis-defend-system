{
    "name": "tsal/alexis",
    "description": "Defend system for Laravel",
    "type": "library",
    "version": "v1.0.3-beta",
    "minimum-stability": "beta",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tsal\\Alexis\\": "src/"
        }
